package Java_Basics;

public class Variables {

	public static void main(String[] args) {
	//declaring variable
		int x;
		// assigns value of 4 to x
		x = 4;
		System.out.println(x);
		//reassign the variable
		x = 43;
		System.out.println(x);
		
		//declare variable type words
		String words;
		words = "This is a sentence";
		System.out.println(words);
	}

}
